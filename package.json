{
  "name": "@geoweb/datamodels",
  "version": "1.0.0",
  "description": "Ceramic Data Models for the Geo Web",
  "scripts": {
    "types": "del types && json2ts -i schemas -o types",
    "create-model": "node --experimental-json-modules ./scripts/create-model.mjs",
    "publish-model": "node --experimental-json-modules ./scripts/publish-model.mjs"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "schema-org-ceramic": "^1.0.2",
    "@glazed/types": "^0.1.3",
    "@ceramicnetwork/http-client": "^1.5.7",
    "@glazed/devtools": "^0.1.3",
    "dids": "^2.4.3",
    "key-did-provider-ed25519": "^1.1.0",
    "key-did-resolver": "^1.4.4",
    "lodash": "^4.17.21",
    "uint8arrays": "^3.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "main": "dist/model.cjs",
  "module": "dist/model.mjs",
  "typings": "dist/model.d.ts",
  "exports": {
    "import": "./dist/model.mjs",
    "require": "./dist/model.cjs",
    "default": "./dist/model.cjs"
  }
}
